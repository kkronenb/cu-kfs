  <bean id="SubObjectCodeCurrent" parent="SubObjectCodeCurrent-parentBean" />
  <bean id="SubObjectCodeCurrent-parentBean" parent="SubObjectCode" abstract="true">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.SubObjectCodeCurrent"/>
    <property name="inquiryDefinition">
    	<bean parent="SubObjectCodeCurrent-inquiryDefinition" />
    </property>
    <property name="lookupDefinition">
    	 <bean parent="SubObjectCodeCurrent-lookupDefinition"/>
   	 </property>
    <property name="objectLabel" value="Sub-Object Code Current"/>
    <property name="attributes">
      <list>
        <bean parent="ObjectCode-universityFiscalYear"/>
        <bean parent="ObjectCode-chartOfAccountsCode"/>
        <bean parent="ObjectCode-financialObjectCode"/>
        <bean parent="Account-accountNumber"/>
        <bean parent="SubObjectCode-financialSubObjectCode"/>
        <bean parent="SubObjectCode-financialSubObjectCodeName"/>
        <bean parent="SubObjectCode-financialSubObjectCdshortNm"/>
        <bean parent="SubObjectCode-active"/>
      </list>
    </property>
  </bean>
  
  <bean id="SubObjectCodeCurrent-inquiryDefinition" parent="SubObjectCodeCurrent-inquiryDefinition-parentBean" />

	<bean id="SubObjectCodeCurrent-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="title" value="Sub-Object Code Inquiry" />
		<property name="inquirySections">
			<list>
				<ref bean="SubObjectCodeCurrent-inquirySectionDefinition" />
			</list>
		</property>
	</bean>
  
  <bean id="SubObjectCodeCurrent-inquirySectionDefinition" parent="SubObjectCodeCurrent-inquirySectionDefinition-parentBean" />
	<bean id="SubObjectCodeCurrent-inquirySectionDefinition-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="universityFiscalYear" />
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="financialObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName" />
				<bean parent="FieldDefinition" p:attributeName="active" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCdshortNm" />
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->

  <bean id="SubObjectCodeCurrent-lookupDefinition" parent="SubObjectCodeCurrent-lookupDefinition-parentBean" />
  <bean id="SubObjectCodeCurrent-lookupDefinition-parentBean" abstract="true" parent="SubObjectCodeCurrent-lookupDefinition-base-parentBean" />
  <bean id="SubObjectCodeCurrent-lookupDefinition-base-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sub-Object Code Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>financialSubObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" p:defaultValueFinderClass="org.kuali.kfs.sys.businessobject.defaultvalue.FiscalYearFinder"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>
  
</beans>
